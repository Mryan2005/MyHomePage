{
  "version": 2,
  "redirects": [
    {
      "source": "/assets/(.*)",
      "destination": "/browser/assets/$1",
      "permanent": true
    },
    {
      "source": "/(.*)\\.js",
      "destination": "/browser/$1.js",
      "permanent": true
    },
    {
      "source": "/(.*)\\.css",
      "destination": "/browser/$1.css",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/browser/index.html"
    }
  ]
  
}
